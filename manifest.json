{
    "manifest_version": 3,
    "name": "YouTube Timestamp Finder",
    "version": "1.0",
    "description": "Find timestamps in YouTube videos based on user input prompt",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  